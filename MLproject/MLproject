name: loan_approval_project

conda_env: conda.yaml

entry_points:
  main:
    parameters:
      n_estimators: {type: int, default: 100}
      learning_rate: {type: float, default: 0.1}
      dataset : {type: string, default: "data_preprocessing/loan_approval_dataset_preprocessing.csv"}
    command: "python modelling.py {n_estimators} {learning_rate} {dataset}"